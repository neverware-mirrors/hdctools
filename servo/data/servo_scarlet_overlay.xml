<root>
  <include>
    <name>servoflex_v2_r0_p50.xml</name>
  </include>
  <include>
    <name>arm_ec_common.xml</name>
  </include>
  <include>
    <name>cr50.xml</name>
  </include>
  <include>
    <name>detachable.xml</name>
  </include>
  <include>
    <name>warm_reset_reconf.xml</name>
  </include>
  <control>
    <name>fw_up</name>
    <alias>ec_boot_mode</alias>
    <doc>STM32 EC boot0 control</doc>
    <params clobber_ok= "" interface="2" drv="tca6416" slv="0x20" od="PU"
    port="1" offset="7" map="onoff_i" init="off"/>
  </control>
  <control>
    <name>power_state</name>
    <doc>Used to turn the DUT off and on</doc>
    <params clobber_ok="" shutdown_delay="11.0" reset_recovery="0.5"/>
  </control>
  <control>
    <name>lid_open</name>
    <doc>fake lid for products without one</doc>
    <params subtype="not_applicable" interface="servo" drv="na" map="na"
    clobber_ok=""></params>
  </control>
</root>
