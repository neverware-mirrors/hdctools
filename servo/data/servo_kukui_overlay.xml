<root>
  <include>
    <name>servoflex_v2_r0_p50.xml</name>
  </include>
  <include>
    <name>arm_ec_common.xml</name>
  </include>
  <include>
    <name>cr50.xml</name>
  </include>
  <include>
    <name>detachable.xml</name>
  </include>
  <control>
    <name>ec_chip</name>
    <doc>EC chip name (read-only)</doc>
    <params clobber_ok="" cmd="get" subtype="chip" interface="servo"
    drv="cros_chip" chip="stm32"/>
  </control>
  <control>
    <name>cold_reset</name>
    <doc>cold reset used for warm reset</doc>
    <remap>warm_reset</remap>
  </control>
  <control>
    <name>power_state</name>
    <doc>Used to turn the DUT off and on</doc>
    <params clobber_ok="" shutdown_delay="3.0" reset_recovery="2.0"
    warm_reset_can_hold_ap="no" wait_ext_is_fake="yes"/>
  </control>
  <control>
    <name>lid_open</name>
    <doc>fake lid for products without one</doc>
    <params subtype="not_applicable" interface="servo" drv="na" map="na"
    clobber_ok=""></params>
  </control>
</root>
